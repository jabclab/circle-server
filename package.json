    "sinon": "~1.6.0",
    "chai": "~1.5.0",
    "sinon-chai": "~2.3.1",
    "grunt": "~0.4.0",
    "grunt-env": "~0.2.1",
    "grunt-complexity": "~0.1.0",
    "grunt-simple-mocha": "~0.3.2",
    "grunt-blanket": "0.0.1",
    "grunt-contrib-jshint": "~0.2.0",
    "grunt-docco": "~0.2.0"
